<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html b:version='2' class='v2' expr:dir='data:blog.languageDirection' xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr' expr:lang='data:blog.locale'>
	<head>
		<meta expr:charset='data:blog.encoding'/>
		<meta http-equiv='X-UA-Compatible' content='IE=edge'/>
    <meta name='viewport' content='width=device-width, initial-scale=1'/>
		
		<!-- all-head-contents -->
		<b:include data='blog' name='all-head-content'/>
		<!-- /all-head-contents -->
		
		<title><data:blog.pageTitle/></title>

		<!-- Default CSS -->
		&lt;!--<style/>--&gt;
		<b:skin><![CDATA[
		/* ADD YOUR CSS HERE */
		.hidden {display: none !important}
		]]></b:skin>
		<!-- /Default CSS -->

		<b:include data='blog' name='google-analytics'/>
	</head>
	<body>
		<b:section class='navbar' id='navbar' maxwidgets='1' name='Navbar' showaddelement='no'>
      <b:widget id='Navbar1' locked='true' title='Thanh điều hướng' type='Navbar' visible='true'/>
    </b:section>

    <b:if cond='data:blog.pageType == &quot;index&quot;'>
      <div itemscope='itemscope' itemtype='http://schema.org/Blog' class='hidden'>
        <meta expr:content='data:blog.title' itemprop='name'/>
        <b:if cond='data:blog.metaDescription'>
          <meta expr:content='data:blog.metaDescription' itemprop='description'/>
        </b:if>
      </div>
    </b:if>
        
    <!-- START YOUR CODE HERE -->

	<!-- Default JavaScript -->
	&lt;!--</body>--&gt;
	<!-- /Default JavaScript -->
	&lt;/body&gt;
</html>
